{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Stream": {
    "Source": "http://192.168.1.123/webcam/?action=stream&advance_headers=1",
    "TargetFps": 6,
    "BitrateKbps": 800,
    "Local": {
      "Enabled": false
    },
    "Mix": {
      "Enabled": false
    },
    "Camera": {
      "Disabled": true
    },
    "Console": {
      "Enabled": true,
      "AllowSend": false,
      "MaxLines": 500,
      "RateLimit": {
        "CommandsPerMinute": 20
      },
      "DisallowedCommands": [
        "M112",
        "M108"
      ],
      "RequireConfirmation": [
        "M112"
      ]
    }
  },
  "Moonraker": {
    "BaseUrl": "http://192.168.1.123:7125/",
    "ApiKey": "",
    "AuthHeader": "X-Api-Key"
  },
  "Audio": {
    "Folder": "/home/you/PrintStreamerData/audio",
    "Enabled": false
  },
  "Overlay": {
    "Enabled": false,
    "RefreshMs": 500,
    "Template": "Nozzle: {nozzle:0}C/{nozzleTarget:0}C | Bed: {bed:0}C/{bedTarget:0}C | Layer {layers} | {progress:0}%\nSpeed:{speed}mm/s | Flow:{flow} | Fil:{filament}m | ETA:{eta:hh:mm tt}\nFilament: {filament_brand} {filament_color}\n{song}",
    "FontSize": 16,
    "FontColor": "white",
    "BoxColor": "black@0.4",
    "BoxHeight": 75,
    "X": "0",
    "Y": "480-75",
    "Box": true,
    "BoxBorderW": 2,
    "PadSpeedWidth": 3,
    "PadFlowWidth": 5,
    "Timezone": "America/New_York"
  },
  "YouTube": {
    "OAuth": {
      "ClientSecretsFilePath": "/home/you/.printstreamer/client_secret.json",
      "TokenFile": "/home/you/.printstreamer/youtube_token.json",
      "ClientId": "",
      "ClientSecret": "",
      "RefreshToken": ""
    },
    "LiveBroadcast": {
      "Enabled": false,
      "EndStreamAfterPrint": false,
      "Title": "3D Printer Live Stream",
      "Description": "Live stream from my Flashforge AD5M Pro.",
      "Privacy": "unlisted",
      "CategoryId": "28"
    },
    "Playlist": {
      "Name": "PrintStreamer",
      "Privacy": "public"
    },
    "TimelapseUpload": {
      "Enabled": false,
      "Privacy": "unlisted",
      "CategoryId": "28"
    },
    "Polling": {
      "_comment": "YouTube API polling configuration to reduce quota usage",
      "Enabled": true,
      "BaseIntervalSeconds": 15,
      "MinIntervalSeconds": 10,
      "MaxIntervalSeconds": 60,
      "IdleThresholdMinutes": 5,
      "BackoffMultiplier": 1.5,
      "MaxJitterSeconds": 5,
      "RequestsPerMinute": 100,
      "CacheDurationSeconds": 5
    }
    ,
    "Reuse": {
      "Enabled": true,
      "StoreFile": "youtube_reuse_store.json",
      "TtlMinutes": 1440,
      "OnlyUnlistedOrPrivateForReuse": true
    }
  },
  "Timelapse": {
    "MainFolder": "/home/you/PrintStreamerData/timelapse",
    "Period": "00:00:15",
    "LastLayerOffset": 1
